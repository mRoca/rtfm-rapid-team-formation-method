  "bugs": {
    "url": "https://github.com/mRoca/rtfm-rapid-team-formation-method/issues"
  },
  "homepage": "https://github.com/mRoca/rtfm-rapid-team-formation-method#readme",
  "devDependencies": {
    "markdown-to-slides": "^1.0.3"
  }
}
